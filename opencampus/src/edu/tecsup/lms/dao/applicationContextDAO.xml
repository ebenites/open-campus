	<bean id="fichaDAO" class="edu.tecsup.lms.dao.FichaDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>

	<bean id="fichaHerramientaDAO"
		class="edu.tecsup.lms.dao.FichaHerramientaDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>

	<bean id="fichaRecursoDAO"
		class="edu.tecsup.lms.dao.FichaRecursoDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>

	<bean id="fichaUnidadDAO"
		class="edu.tecsup.lms.dao.FichaUnidadDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>

	<bean id="fichaUnidadRecursoDAO"
		class="edu.tecsup.lms.dao.FichaUnidadRecursoDAO">
		<property name="dataSource" ref="dataSource" />
	</bean>

	<bean id="jerarquiaDAO" class="edu.tecsup.lms.dao.JerarquiaDAO">
		<property name="dataSource" ref="dataSource" />
