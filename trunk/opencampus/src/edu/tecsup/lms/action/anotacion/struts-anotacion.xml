<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="campus-anotacion" extends="campus-default" namespace="/anotacion">
	
		<action name="CargarPortada" method="cargarPortada" 
			class="edu.tecsup.lms.action.anotacion.AnotacionAction">
			<result name="success">/comun/portal/servicio_apuntes.jsp</result>
			<interceptor-ref name="usuario-campus"></interceptor-ref>
		</action>
	
		<action name="Anotacion" method="inicio" class="edu.tecsup.lms.action.anotacion.AnotacionAction">
			<result name="success">/comun/anotacion/anotacion.jsp</result>
			<interceptor-ref name="usuario-campus"></interceptor-ref>
		</action>
		
		<action name="Insertar" method="insertar" class="edu.tecsup.lms.action.anotacion.AnotacionAction">			
			<interceptor-ref name="usuario-campus"></interceptor-ref>			
		</action>
		
		<action name="Eliminar" method="eliminar" class="edu.tecsup.lms.action.anotacion.AnotacionAction">			
			<interceptor-ref name="usuario-campus"></interceptor-ref>			
		</action>
		
		<action name="Actualizar" method="actualizar" class="edu.tecsup.lms.action.anotacion.AnotacionAction">			
			<interceptor-ref name="usuario-campus"></interceptor-ref>			
		</action>
		
	</package>

</struts>